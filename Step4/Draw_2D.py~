import os
import numpy as np
import matplotlib.pyplot as plt
from mpl_toolkits.mplot3d import Axes3D

def Draw_Graph_2D():
	
	x = [1, 2, 3, 4]
	y = [1, 4, 9, 6]

	# plt.plot(x, y) # draw line
	plt.plot(x, y, 'ko') # draw dots
	plt.margins(0.2)
	
	plt.xlabel('sequence')
	plt.ylabel('V')


	plt.axhline(y=3, color='k', ls='dashed')
	plt.axhline(y=4, color='k', ls='dashed')

	plt.show()

if __name__ == '__main__':
	Draw_Graph_2D()
